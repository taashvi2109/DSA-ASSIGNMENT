#include <iostream>
using namespace std;

#define SIZE 100

class Stack {
    int arr[SIZE];
    int top;

public:
    Stack() {
        top = -1;
    }

    void push(int x) {
        if (top == SIZE - 1)
            cout << "Stack Overflow\n";
        else
            arr[++top] = x;
    }

    void pop() {
        if (top == -1)
            cout << "Stack Underflow\n";
        else
            top--;
    }

    bool isEmpty() {
        return top == -1;
    }

    bool isFull() {
        return top == SIZE - 1;
    }

    void display() {
        if (top == -1)
            cout << "Stack is Empty\n";
        else {
            for (int i = top; i >= 0; i--)
                cout << arr[i] << " ";
            cout << endl;
        }
    }

    int peek() {
        if (top == -1)
            return -1;
        return arr[top];
    }
};

int main() {
    Stack s;
    int choice, value;

    while (true) {
        cout << "\n1. Push\n2. Pop\n3. isEmpty\n4. isFull\n5. Display\n6. Peek\n7. Exit\n";
        cin >> choice;

        switch (choice) {
            case 1:
                cin >> value;
                s.push(value);
                break;

            case 2:
                s.pop();
                break;

            case 3:
                cout << (s.isEmpty() ? "Empty\n" : "Not Empty\n");
                break;

            case 4:
                cout << (s.isFull() ? "Full\n" : "Not Full\n");
                break;

            case 5:
                s.display();
                break;

            case 6:
                cout << s.peek() << endl;
                break;

            case 7:
                return 0;
        }
    }
}
